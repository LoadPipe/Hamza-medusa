{
    "name": "hamza-client",
    "version": "1.2.2",
    "private": true,
    "author": "Garo Nazarian, Beau Troxclair, Jonny Bajada, Karan Chugh, Chris Li, John R. Kosinski",
    "description": "Hamza Web Client",
    "keywords": [
        "medusa-storefront"
    ],
    "scripts": {
        "_devcomment": "This allows for debugging server components. The debugger will be attached to port 9230.  In LoadpipeSetup/market/hamza-client.code-workspace, There is a companion configuration to manage server component debug with breakpoints.  There are 2 debuggers, one for client and another for server.  Both should work out of the box.",
        "dev": "NODE_OPTIONS='--inspect' next dev -p 8000",
        "build": "next build",
        "start": "next start -p 8000",
        "lint": "next lint",
        "cypress": "cypress open",
        "analyze": "ANALYZE=true next build",
        "normalclean": "chmod +x ./scripts/clean.sh && ./scripts/clean.sh",
        "softclean": "chmod +x ./scripts/softclean.sh && ./scripts/softclean.sh",
        "deepclean": "chmod +x ./scripts/deepclean.sh && ./scripts/deepclean.sh",
        "import-currencies": "chmod +x ./scripts/import-currencies.sh && ./scripts/import-currencies.sh",
        "e2e:chrome:all": "cypress run --browser chrome",
        "e2e:chrome:headless": "cypress run --browser chrome --spec cypress/e2e/headless",
        "e2e:firefox:all": "cypress run --browser firefox",
        "e2e:firefox:headless": "cypress run --browser firefox --spec cypress/e2e/headless",
        "e2e:chrome:file": "cypress run --browser chrome --spec",
        "e2e:chrome:headed:file": "cypress run --browser chrome --headed --spec"
    },
    "resolutions": {
        "webpack": "^5",
        "@types/react": "17.0.40"
    },
